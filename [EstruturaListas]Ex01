using System;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            // Faça um programa que leia 5 números e informe o maior número. . 
            double[] numeros = { 10, 20, 40, 50, 80 };
        
            foreach (double i in numeros)
            {
                Console.WriteLine("{0} ", i);
            }
        }
    }
}
                     
    

